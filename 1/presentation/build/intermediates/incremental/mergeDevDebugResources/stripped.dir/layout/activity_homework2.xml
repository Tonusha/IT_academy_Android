<?xml version="1.0" encoding="utf-8"?>
<HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="580dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:orientation="vertical"
                android:weightSum="10">

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="#efecd4"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:text="@string/austrian"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="270dp"
                android:background="@color/red"
                android:orientation="vertical"
                android:weightSum="90">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:layout_marginTop="90dp"
                    android:background="@color/white"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:weightSum="30" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="580dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:orientation="vertical"
                android:weightSum="10">

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="#efecd4"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:text="@string/poland"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="270dp"
                android:background="@color/white"
                android:orientation="vertical"
                android:weightSum="90">

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="135dp"
                    android:layout_marginTop="135dp"
                    android:background="@color/red"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:weightSum="30" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="580dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:orientation="vertical"
                android:weightSum="10">

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="#efecd4"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:text="@string/italy"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:weightSum="90">

                <LinearLayout
                    android:layout_width="193.3dp"
                    android:layout_height="match_parent"
                    android:background="@color/green"
                    android:orientation="horizontal"
                    android:weightSum="30"></LinearLayout>

                <LinearLayout
                    android:layout_width="193.3dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    android:orientation="horizontal"
                    android:weightSum="30"></LinearLayout>

                <LinearLayout
                    android:layout_width="193.3dp"
                    android:layout_height="match_parent"
                    android:background="@color/red"
                    android:orientation="horizontal"
                    android:weightSum="30"></LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="580dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:orientation="vertical"
                android:weightSum="10">

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="#efecd4"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:text="@string/colombia"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="270dp"
                android:background="@color/yellow"
                android:orientation="vertical"
                android:weightSum="90">

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="67.5dp"
                    android:layout_marginTop="135dp"
                    android:background="@color/navy"
                    android:orientation="vertical"
                    android:weightSum="30" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="67.5dp"
                    android:background="@color/red"
                    android:orientation="vertical"
                    android:weightSum="30" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="580dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:orientation="vertical"
                android:weightSum="10">

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="#efecd4"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:text="@string/madagascar"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:weightSum="90">

                <LinearLayout
                    android:layout_width="170dp"
                    android:layout_height="match_parent"
                    android:background="@color/white"
                    android:orientation="horizontal"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:weightSum="90">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:background="@color/red"
                        android:orientation="vertical"
                        android:weightSum="50"></LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:background="@color/green"
                        android:orientation="horizontal"
                        android:weightSum="50">

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="580dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:orientation="vertical"
                android:weightSum="10">

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="#efecd4"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:text="@string/thailand"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="270dp"
                android:background="@color/red"
                android:orientation="vertical"
                android:weightSum="90">

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_marginTop="45dp"
                    android:background="@color/white"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:weightSum="30" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="match_parent"
                    android:layout_height="90dp"
                    android:background="@color/navy"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:weightSum="30" />

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:background="@color/white"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:weightSum="30" />

            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="580dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:orientation="vertical"
                android:weightSum="10">

                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="#efecd4"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:text="@string/denmark"
                    android:textColor="@color/black" />
            </LinearLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="270dp"
                android:background="@color/red"
                android:orientation="vertical"
                android:weightSum="90">

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginTop="115dp"
                    android:background="@color/white"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:weightSum="30" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="40dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="155dp"
                    android:background="@color/white"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:weightSum="30" />

            </FrameLayout>

        </LinearLayout>

    </LinearLayout>


</HorizontalScrollView>